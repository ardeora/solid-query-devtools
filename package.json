{
  "version": "0.0.1",
  "name": "@adeora/solid-query-devtools",
  "description": "TanStack Solid Query Devtools",
  "license": "MIT",
  "author": "Aryan Deora",
  "contributors": [],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ardeora/solid-query-devtools.git"
  },
  "homepage": "https://github.com/ardeora/solid-query-devtools#readme",
  "bugs": {
    "url": "https://github.com/ardeora/solid-query-devtools/issues"
  },
  "files": [
    "src",
    "dist"
  ],
  "sideEffects": [
    "./src/fonts.ts"
  ],
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "browser": {},
  "exports": {
    "solid": {
      "development": "./dist/dev.jsx",
      "import": "./dist/index.jsx"
    },
    "development": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/dev.js"
      },
      "require": "./dist/dev.cjs"
    },
    "import": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "require": "./dist/index.cjs"
  },
  "typesVersions": {},
  "scripts": {
    "dev": "vite serve dev",
    "build": "tsup",
    "test": "concurrently pnpm:test:*",
    "test:client": "vitest",
    "test:ssr": "pnpm run test:client --mode ssr",
    "prepublishOnly": "pnpm build",
    "format": "prettier --ignore-path .gitignore -w \"src/**/*.{js,ts,json,css,tsx,jsx}\" \"dev/**/*.{js,ts,json,css,tsx,jsx}\"",
    "update-deps": "pnpm up -Li",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "solid-js": "^1.6.0",
    "@tanstack/solid-query": "^5.0.0-alpha.15"
  },
  "dependencies": {
    "@emotion/css": "^11.10.5",
    "@solid-primitives/deep": "^0.0.102",
    "@solid-primitives/keyed": "^1.1.4",
    "@tanstack/match-sorter-utils": "^8.8.4",
    "solid-transition-group": "^0.2.2",
    "superjson": "^1.12.1"
  },
  "devDependencies": {
    "concurrently": "^7.6.0",
    "esbuild": "^0.17.4",
    "esbuild-plugin-solid": "^0.5.0",
    "jsdom": "^21.1.0",
    "prettier": "2.8.3",
    "solid-js": "^1.6.9",
    "@tanstack/solid-query": "^5.0.0-alpha.15",
    "tsup": "^6.5.0",
    "tsup-preset-solid": "^0.1.4",
    "typescript": "^4.9.4",
    "vite": "^4.0.4",
    "vite-plugin-solid": "^2.5.0",
    "vitest": "^0.28.2"
  },
  "keywords": [
    "solid"
  ],
  "packageManager": "pnpm@7.22.0"
}
